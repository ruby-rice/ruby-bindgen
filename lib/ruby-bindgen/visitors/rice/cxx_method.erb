<%- if cursor.static? -%>
<%= is_template && signature && !signature.empty? ? "template " : "" %>define_singleton_function<%= signature %>("<%= name %>", &<%= cursor.semantic_parent.qualified_display_name %>::<%= cursor.spelling %><%= args.empty? ? ")" : ",\n  #{args.join(", ")})" %><%= return_buffer ? ".\n  define_return(ReturnBuffer())" : "" %>
<%- else -%>
<%= is_template && signature && !signature.empty? ? "template " : "" %>define_method<%= signature %>("<%= name %>", &<%= cursor.semantic_parent.qualified_display_name %>::<%= cursor.spelling %><%= args.empty? ? ")" : ",\n  #{args.join(", ")})" %><%= return_buffer ? ".\n  define_return(ReturnBuffer())" : "" %>
<%- end -%>
